set xlabel "Time (sec)"
set ylabel "Throughout (Mbps)"
set grid xtics
set grid ytics
set yrange [-50:1100]

set terminal postscript eps size 6.4,4.8 enhanced color font 'Helvetica,25'
set output "vmmig_tcp_plot.eps"

plot "plotdata_noopt" u 1:2 title 'Throughput during VM migration with no optimization' w l lt 1 lw 3 lc rgb "red",\
     "plotdata_opt" u 1:2 title 'Throughput during VM migration with optimization' w l lt 1 lw 3 lc rgb "blue"
